    "p = 26513\n",
    "q = 32321\n",
    "\n",
    "#see https://cp-algorithms.com/algebra/extended-euclid-algorithm.html\n",
    "def extended_gcd(a, b):\n",
    "    if b == 0: return 1, 0, a\n",
    "    x, y, gcd = extended_gcd(b, a % b)\n",
    "    return y, x - y * (a // b), gcd\n",
    "extended_gcd(26513, 32321)"
